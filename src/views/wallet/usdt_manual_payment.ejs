   <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
   
   <script>
    // Example USDT Wallet Address (Ethereum-based)
    const usdtWalletAddress = "<%=UsdtWalletAddress%>";  // Replace with your actual USDT wallet address

    // Construct the Ethereum wallet link (USDT ERC20)
    // const usdtLink = `ethereum:${usdtWalletAddress}`;
    
    const usdtLink = `${usdtWalletAddress}`;

    // Function to load the QR Code using the USDT wallet address link
    function generateQRCode(link) {
      // Use an external QR code generation service
      const qrCodeImgSrc = `https://api.qrserver.com/v1/create-qr-code/?size=250x250&data=${encodeURIComponent(link)}`;
      
      // Set the QR Code image source
      document.getElementById("crypto-qr-code").src = qrCodeImgSrc;
    }

    // Generate the QR Code when the page loads
    generateQRCode(usdtLink);
  </script>
  
    <script>
    
    window.onload = function() {
    // Set the timer duration in seconds (7 days)
    var duration = 7 * 24 * 60 * 60;
    var timerDisplay = document.querySelector(".timerusdt h2");
    var countdown = duration, days, hours, minutes, seconds;

    function startCountdown() {
        function updateTimer() {
            days = Math.floor(countdown / (24 * 60 * 60));
            hours = Math.floor((countdown % (24 * 60 * 60)) / (60 * 60));
            minutes = Math.floor((countdown % (60 * 60)) / 60);
            seconds = Math.floor(countdown % 60);

            days = days < 10 ? "0" + days : days;
            hours = hours < 10 ? "0" + hours : hours;
            minutes = minutes < 10 ? "0" + minutes : minutes;
            seconds = seconds < 10 ? "0" + seconds : seconds;

            timerDisplay.innerHTML = `<span>${days}</span> : <span>${hours}</span> : <span>${minutes}</span> : <span>${seconds}</span>`;

            if (countdown > 0) {
                countdown--;
                setTimeout(updateTimer, 1000); // Call updateTimer every second
            } else {
                alert("timeout");
                window.history.back();
            }
        }
        updateTimer(); // Initial call to start the countdown
    }

    startCountdown();
};

    </script>
   <script>
      // Function to show notifications
      function showNotification(message) {
         const notification = document.getElementById('notification');
         notification.textContent = message;
         notification.classList.add('show');
         setTimeout(() => {
            notification.classList.remove('show');
         }, 2000);
      }

      const copyUpiId = document.getElementById("copy_wallet_address_btn");

      copyUpiId.addEventListener("click", function () {
         const walletAddress = document.getElementById("wallet_address_field").innerText;
         navigator.clipboard.writeText(walletAddress).then(() => {
            showNotification("Wallet address copied to clipboard!");
         }).catch(() => {
            showNotification("Failed to copy wallet address!");
         });
      });

      const alertMessage = (text) => {
         // Create alert message div
         const msg = document.createElement('div');
         msg.className = 'alert-message';  // Apply the alert message styling

         // Set the text content
         msg.textContent = text;

         // Append the message to the body
         document.body.appendChild(msg);

         // Remove the message after the animation is finished (3 seconds)
         setTimeout(() => {
            document.body.removeChild(msg);
         }, 3000);  // Matches the 3s animation duration
      }
      const submitButton = document.getElementById("submit_button");
      submitButton.addEventListener("click", async (event) => {
         try {
            event.preventDefault();

            // Get the reference number value
            const refNo = document.getElementById("ref_no").value;
            const amount = "<%=Amount%>";

            // Check if the reference number is empty
            if (!refNo || refNo.trim() === "") {
               alertMessage("Reference number cannot be empty!");
               return;  // Stop further execution
            }

            console.log(amount);
            const response = await axios.post(
               "/wallet/paynow/manualusdtrequest",
               {
                  money: amount,
                  utr: refNo,
               },
            );

            if (!response?.data) {
               alertMessage("Something went wrong!");
               return;
            }

            if (response?.data?.status === false) {
               alertMessage(response?.data?.message);
               return;
            }

            alertMessage(response?.data?.message);

            window.location.href = "/wallet/rechargerecord";
         } catch (error) {
            console.log(error);
            alertMessage("aryan Something went wrong!");
         }
      });
   </script>
