    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.7.7/axios.min.js"
        integrity="sha512-DdX/YwF5e41Ok+AI81HI8f5/5UsoxCVT9GKYZRIzpLxb8Twz4ZwPPX+jQMwMhNQ9b5+zDEefc+dcvQoPWGNZ3g=="
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script>
            let page = 1;  
            const limit = 10;
            const tabList = document.getElementById('tablist');
            let type;
            let isLoading = false; // Prevent multiple fetches at the same time
            let hasMoreData = true; // To track if thereâ€™s more data to load
            let additionConstant = 0;
        
            const displayActiveText = (item) => {
                const span = item.querySelector('span .tabDiv');
                if (span) {
                    type = span.innerText.toLowerCase();
                }
            };
        
            const fetchData = async () => {
                if (isLoading || !hasMoreData) return; // Prevent duplicate requests
        
                isLoading = true;
                try {
                    const response = await axios.post('/api/webapi/withdraw/list', { 
                        type: type,
                        page: page,
                        limit: limit,
                    });
                    const data = response.data.datas;
        
                    if (data.lists && data.lists.length > 0) {
                        displayHtml(data.lists);
                        page++; // Increment page for the next request
                    } else {
                        hasMoreData = false; // No more data to load
                        const container = document.querySelector('#data');
                        container.innerHTML += `
                            <div data-v-1bb72b7d="" class="infiniteScroll__loading">
                                <div data-v-1bb72b7d="">No more</div>
                            </div>
                        `;
                    }
                } catch (error) {
                    console.error('Error fetching data:', error);
                } finally {
                    isLoading = false; // Allow further requests
                }
            };
            
            const formateT = (params) => {
            let result = (params < 10) ? "0" + params : params;
            return result;
        }
            
            const timerJoin = (params = '') => {
            let date = '';
            if (params) {
                date = new Date(Number(params));
            } else {
                date = new Date();
            }
            let years = formateT(date.getFullYear());
            let months = formateT(date.getMonth() + 1);
            let days = formateT(date.getDate());

            let hours = formateT(date.getHours());
            let minutes = formateT(date.getMinutes());
            let seconds = formateT(date.getSeconds());
            return years + '-' + months + '-' + days + ' ' + hours + '-' + minutes + '-' + seconds;
        }
        
            const displayHtml = (items) => {
                const container = document.querySelector('#data');
        
                // Create HTML for the new items
                const gameStatsDiv = items.map((data) => `
                    <div data-v-1320a495="" class="rechargeh__container-content__item">
                        <div data-v-1320a495="" class="rechargeh__container-content__item-header ar-1px-b">
                            <span data-v-1320a495="">Withdraw</span>
                            <span data-v-1320a495="" style="color: ${(data.status == 1) ? '#21976b' : 'rgb(242, 65, 59)'}" class="stateG">
                                ${(data.status == 1) ? 'Completed' : (data.status == 2) ? 'Cancelled' : 'Pending'}
                            </span>
                        </div>
                        <div data-v-1320a495="" class="rechargeh__container-content__item-body">
                            <div data-v-1320a495="">
                                <span data-v-1320a495="">Balance</span>
                                <span data-v-1320a495="">â‚¹${data.money}.00</span>
                            </div>
                            <div data-v-1320a495="">
                                <span data-v-1320a495="">Type</span>
                                <span data-v-1320a495=""> BANK CARD</span>
                            </div>
                            <div data-v-1320a495="">
                                <span data-v-1320a495="">Time</span>
                                <span data-v-1320a495="">${timerJoin(data.time)}</span>
                            </div>
                            <div data-v-1320a495="">
                                <span data-v-1320a495="">Order number</span>
                                <span data-clipboard-text="${data.id_order}" data-v-1320a495="" class="tag-read">
                                    <div data-v-4e585e69="" data-clipboard-text="${data.id_order}" class="tag-read"> ${data.id_order} 
                                        <img data-v-4e585e69="" width="18px" height="15px" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYBAMAAAASWSDLAAAAG1BMVEUAAAD////////////////////////////////rTT7CAAAACHRSTlMAf2/fzyAQv1i/o5gAAABWSURBVBjTY0AHSYJgoApis1p0gEFzApDD1AKR8VAAchgbIaolBIA6xNvBOkAchA4gB6EDyEHooBOn0EMBweloDkCSUUP2ALrX2GBeCADJB0Fk1NCDCQANHCEHp1BAOwAAAABJRU5ErkJggg==" class="m-l-5">
                                    </div>
                                </span>
                            </div>
                            <div data-v-1320a495="">
                                <span data-v-1320a495="">Remarks</span>
                            </div>
                            <div data-v-1320a495="">
                                <textarea data-v-1320a495="" class="textarea" name="remark" cols="30" rows="10" readonly="" style="display: none;"></textarea>
                            </div>
                        </div>
                    </div>
                `).join('');
        
                // Append the new div to the container
                container.innerHTML += gameStatsDiv;
            };
        
            const handleScroll = () => {
            const addition = (window.scrollY + window.innerHeight);
            const scrollHeight = (document.body.scrollHeight - 1);

            if (addition > scrollHeight && additionConstant < addition) {
                additionConstant = addition;
                fetchData();
            }
        };
        
            window.addEventListener('scroll', handleScroll);
        
            // Initial data load
            if (tabList.children.length > 0) {
                tabList.children[0].classList.add('van-tab--active');
                displayActiveText(tabList.children[0]);
                fetchData(); // Load initial data on page load
            }
        
            Array.from(tabList.children).forEach(child => {
                child.addEventListener('click', () => {
                    handleClickActive(child);
                });
            });
        
            const handleClickActive = (item) => {
                Array.from(tabList.children).forEach(child => {
                    child.classList.remove('van-tab--active');
                });
                item.classList.add('van-tab--active');
                displayActiveText(item);
                page = 1; // Reset page when tab changes
                hasMoreData = true; // Reset data loading state
                document.querySelector('#data').innerHTML = ''; // Clear previous data
                fetchData(); // Fetch new data
            };
        
            // Initial data fetch on page load
            fetchData();
        </script>

