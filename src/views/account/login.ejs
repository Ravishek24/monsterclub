    <script>
        function setCookie(cname, cvalue, exdays) {
            const d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            let expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        }

        function alertMess(text) {
            let length = $('.alert-toast .msg').length;
            if (length == 0) {
                $('.alert-toast').append(
                    `
                    <div data-v-1dcba851="" class="msg">
                        <div data-v-1dcba851="" class="msg-content" style=""> ${text} </div>
                    </div>
                    `
                );
                setTimeout(() => {
                    $('.msg').fadeOut();
                    setTimeout(() => {
                        $('.alert-toast .msg').remove();
                    }, 100);
                }, 1500);
            }
        }
        function validateForm(username, password, text) {
            if (!username || !password) {
                alertMess(text);
                return false;
            } else {
                return true;
            }
        }

              $('button').click(async (e) => {
    e.preventDefault();
    let username = $('input:eq(0)').val().trim();
    let password = $('input:eq(1)').val().trim();

    if (username && password) {
        verifyLogin(username, password);
    }
});

function openCaptcha() {
    $('#captchaPopup').css('display', 'flex');
    initializeCaptcha();
}

function closeCaptcha() {
    $('#captchaPopup').css('display', 'none');
}

function initializeCaptcha() {
    var captcha = sliderCaptcha({
        id: 'captcha',
        loadingText: 'Loading...',
        failedText: 'Try again',
        barText: 'Slide right to fill',
        repeatIcon: 'fa fa-redo',
        onSuccess: function () {
            $('.van-overlay').fadeOut(300);
            $('.Loading').fadeIn(10);
            setTimeout(() => {
                location.href = '/home';
            }, 500);
        }
    });
}

function verifyLogin(username, password) {
    $('.van-overlay').fadeIn(10);
    $.ajax({
        type: "POST",
        url: "/api/webapi/login",
        data: {
            username: username,
            pwd: password,
        },
        dataType: "json",
        success: function (response) {
            $('.van-overlay').fadeOut(300);
            if (response.status === true) {
            setCookie('token', response.token, 1); 
            setCookie('auth', response.value, 1);
                openCaptcha();
            } else {
                alertMess(response.message);
            }
        }
    });
}

