        let phoneNumber = '';

        const fetchUser = async () => {
            try {
                const response = await fetch('/api/webapi/GetUserInfo');
                const data = await response.json();
                if (!data || !data.data) throw new Error("User data not found!");

                phoneNumber = encodeURIComponent(data.data[userInfoTypes.phoneNumber]);

                displayGames(); // Display games with images
            } catch (error) {
                console.error('Error fetching user data:', error);
            }
        };

        const displayGames = () => {
            const container = document.getElementById('game-container');
            container.innerHTML = '';

            let gamesToDisplay = [
                ...mTypes.slots.gType0.map(mType => ({ mType, gType: 0 })),
                ...mTypes.slots.gType32.map(mType => ({ mType, gType: 32 }))
            ];



            gamesToDisplay.forEach(game => {
           
            const gameUrl = `https://sxm.in.net/jdb/post?iv=f1ab7cea8106a3e4&key=b4d70df8d5c2857c&uid=${phoneNumber}&serverUrl=https://jdb.Damantrade.online&parent=sxmapis&gType=${game.gType}&mType=${game.mType}`;
            
            
             //   const gameUrl = `https://fluxapis.live/jdb/post?iv=05bae19880a21dd1&key=3467d9770b6bdd41&uid
             // =${phoneNumber}&serverUrl=https://api2.dubailottery.club/&parent=digexpert&gType=${game.gType}&mType   
             // =${game.mType}`;

                // Define image URL based on gType
                const imageUrl = game.gType === 32
                    ? imageMapping[game.mType] || 'https://via.placeholder.com/120x163' // Use mapping for gType 32
                    : `https://fluxapis.live/jdbimages/${game.mType}.png`; // Default path for other game types

                const gameItem = document.createElement('div');
                gameItem.className = 'game-item';

                const img = new Image();
                img.src = imageUrl;
                img.className = 'game-image';
                img.onerror = () => {
                    img.src = 'https://via.placeholder.com/120x163'; // Placeholder image in case of an error
                };

                const gameLink = document.createElement('a');
                gameLink.href = gameUrl;
                gameLink.className = 'game-link';
                gameLink.appendChild(img);

                gameItem.appendChild(gameLink);
                container.appendChild(gameItem);
            });
        };

        fetchUser();
    </script>
